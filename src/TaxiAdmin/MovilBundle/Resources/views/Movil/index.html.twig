{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Movil list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Id</th>
                <th>Matricula</th>
                <th>Marca</th>
                <th>Modelo</th>
                <th>Anio</th>
                <th>Numchasis</th>
                <th>Combustible</th>
                <th>Nummovil</th>
                <th>Despacho</th>
                <th>Fechaalta</th>
                <th>Fechabaja</th>
                <th>Habilitado</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('movil_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.matricula }}</td>
                <td>{{ entity.marca }}</td>
                <td>{{ entity.modelo }}</td>
                <td>{{ entity.anio }}</td>
                <td>{{ entity.numChasis }}</td>
                <td>{{ entity.combustible }}</td>
                <td>{{ entity.numMovil }}</td>
                <td>{{ entity.despacho }}</td>
                <td>{% if entity.fechaAlta %}{{ entity.fechaAlta|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{% if entity.fechaBaja %}{{ entity.fechaBaja|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{{ entity.habilitado }}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('movil_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('movil_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('movil_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
